{
  "name": "ticket-to-pr",
  "version": "1.4.1",
  "description": "Drag a Notion ticket, get a pull request. AI-powered dev automation.",
  "type": "module",
  "bin": {
    "ticket-to-pr": "bin/ticket-to-pr.js"
  },
  "files": [
    "dist",
    "bin",
    "prompts",
    "projects.example.json",
    "README.md"
  ],
  "homepage": "https://www.tickettopr.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/JohnRiceML/ticket-to-pr.git"
  },
  "bugs": {
    "url": "https://github.com/JohnRiceML/ticket-to-pr/issues"
  },
  "keywords": [
    "notion",
    "claude",
    "ai",
    "pull-request",
    "automation",
    "developer-tools"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run test && npm run build",
    "start": "tsx index.ts",
    "dry-run": "tsx index.ts --dry-run",
    "once": "tsx index.ts --once",
    "init": "tsx index.ts init",
    "doctor": "tsx index.ts doctor",
    "test": "vitest run"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.0",
    "@notionhq/client": "^2.2.0"
  },
  "devDependencies": {
    "typescript": "^5.7.0",
    "tsx": "^4.19.0",
    "@types/node": "^22.0.0",
    "vitest": "^3.0.0"
  }
}
